<% include header %>
	
<% if (entries.length) { %>
<% entries.forEach(function(entry) { %>

<div class="panel panel-warning">
<div class="panel-heading">

<div class="text-muted pull-right">

<%= entry.published %></div>
<%= entry.title %></div>

<div class="panel-body bg-info">
<%= entry.body %>
<%= entry.content %></div>


</div>
</div>
<% }) %>
<% } else { %>
No entries! <a href="/new-entry">Add one!</a>

<% } %>
<% include footer %>